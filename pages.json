{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^kx(.*)": "@/components/kx/$1/index.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "短视频去水印"
			}
		},
		{

			"path": "pages/mdFive/index",
			"style": {
				"navigationBarTitleText": "修改MD5"
			}
		},
		{
			"path": "pages/index/my",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		},
		{
			"path": "pages/my/personalDetails",
			"style": {
				"navigationBarTitleText": "编辑个人资料"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/batch/index",
			"style": {
				"navigationBarTitleText": "批量解析"
			}
		},
		{
			"path": "pages/webview/index",
			"style": {
				"navigationBarTitleText": "教程"
			}
		},
		{
			"path": "pages/my/dataLog",
			"style": {
				"navigationBarTitleText": "历史记录"
			}
		}
	],
	"preloadRule": {
		"pages/index/my": {
			"network": "all",
			"packages": [
				"pages/my"
			]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#FFF",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#303133",
		"selectedColor": "#74dbef",
		"backgroundColor": "#ffffff",
		"list": [
			// 	{
			// 	"text": "首页",
			// 	"pagePath": "pages/index/index",
			// 	"iconPath": "static/image/home.png",
			// 	"selectedIconPath": "static/image/home-h.png"
			// }, 
			{
				"text": "首页",
				"pagePath": "pages/index/index",
				"iconPath": "static/image/home.png",
				"selectedIconPath": "static/image/home-h.png"
			}, {
				"text": "我的",
				"pagePath": "pages/index/my",
				"iconPath": "static/image/self.png",
				"selectedIconPath": "static/image/self-h.png"
			}
		]
	},
	"uniIdRouter": {
		"loginPage": "pages/login/login",
		"needLogin": [
			"pages/index/.*",
			"pages/mdFive/.*",
			"pages/my/.*"
		]
	}
}