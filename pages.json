{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^Zjf(.*)": "@/components/Zjf/$1/index.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/index/my",
			"style": {
				"navigationBarTitleText": "uni-app",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/my/personalDetails",
			"style": {
				"navigationBarTitleText": "编辑个人资料"
			}
		}
	],
	"preloadRule": {
		// "pages/index/my": {
		// 	"network": "all",
		// 	"packages": [
		// 		"pages/my"
		// 	]
		// }
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#303133",
		"selectedColor": "#74dbef",
		"backgroundColor": "#ffffff",
		"list": [{
			"text": "首页",
			"pagePath": "pages/index/index",
			"iconPath": "static/image/home.png",
			"selectedIconPath": "static/image/home-h.png"
		}, {
			"text": "我的",
			"pagePath": "pages/index/my",
			"iconPath": "static/image/self.png",
			"selectedIconPath": "static/image/self-h.png"
		}]
	},
	"uniIdRouter": {}
}